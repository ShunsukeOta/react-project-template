{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "plugins": ["react", "react-hooks", "@typescript-eslint", "prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
        "react": {
        "version": "detect"
        }
    }
}
